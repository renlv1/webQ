      &.isblock
        display none !important
  .swiper-button-next,.swiper-button-prev
    width: 50px
    height: 50px
    position: absolute
    top: 50%
    transform translateY(-50%)
    display block
    cursor pointer
    &.swiper-button-next
      right: -45px
      background: url("../../assets/images/team/arrow-right.svg") no-repeat
