$f2bb24 = #f2bb24
.ourteam-container
  margin: 162px 0 198px
  .wrapper
    width 1260px
    position: relative
    left 50%
    transform translateX(-50%)
    margin-left: 20px
    .swiper-container-horizontal
      overflow: hidden
  @media screen and (max-width: 1400px)
    .pc-wrapper
      display none
    .mobile-wrapper
      margin: 0 auto
      display block
      width: 340px
      margin-left: 0
      .swiper-slide
        width: 340px !important
        margin-right: 0
        box-shadow none !important
        border-top: 6px solid #f2bb24;
        border-bottom: 6px solid #f2bb24;
        border-left: 1px solid rgba(0,0,0,.1)
        border-right: 1px solid rgba(0,0,0,.1)
      .swiper-button-next
        right: -60px !important
  @media screen and (min-width: 1400px)
    .mobile-wrapper
      display none
    .pc-wrapper
      display block
  .container-title
    font-size: 67px
    color #333
    line-height: 1
    text-align: center
    margin-bottom: 146px
    @media screen and (max-width: 600px)
      font-size: 30px
  .swiper-slide
    flex: 1;
    -ms-flex: 0 0 auto;
    //IEdiezaiyiqi
    box-sizing border-box
    width: 360px !important
    height: 720px
    display flex
    flex-direction column
    align-items center
    margin-right: 60px
    transition all .2s linear
    cursor pointer
    .img-w
      height: 164px
      margin: 60px auto 30px;
      display flex
      align-items center
      justify-content center
      &.none-img
        height: 0 !important
        width: 0 !important
        transition all .2s linear
    &:hover
      border-top: 6px solid #f2bb24;
      border-bottom: 6px solid #f2bb24;
      box-shadow: 5px 5px 30px rgba(0,0,0,.1);
    &.active-slide
      border-top: 6px solid #f2bb24;
      border-bottom: 6px solid #f2bb24;
      box-shadow: 5px 5px 30px rgba(0,0,0,.1);
      .img-box
        width: 164px
        height: 164px
        background-color: #d9ab2b
        display flex
        align-items center
        justify-content center
        &::before
          content ''
          display block
          width: 80px
          height: 80px
          background: url("../images/team/b88.svg") no-repeat center / cover
        img
          display none
          //width: 80px
          //height: 80px
      .desc
        //display: -webkit-box;
        //-webkit-box-orient: vertical;
        //-webkit-line-clamp: 7;
        //-ms-box-orident: vertical;
        //-ms-line-clamp: 7;
        //-o-text-overflow: ellipsis;
        height: 166px
        overflow: hidden;
        //text-overflow: ellipsis;
        //word-wrap:break-word;
        //zoom 1
        position: relative
        &::after
          content '...'
          position: absolute
          bottom: 0
          right: 0
      .look-info
        display block
      .is-hidden
        display block !important
        height auto !important
        &::after
          display none
    .img-box
      width: 130px
      height: 130px
      display flex
      align-items center
      justify-content center
      background-color: #333333
      border-radius 50%
      &.none-img
        height: 0 !important
        width: 0 !important
        transition all .2s linear
        img
          width: 0 !important
          height: 0 !important
      img
        width: 60px !important
        height: 60px !important
    .pople-name
      font-size: 20px
