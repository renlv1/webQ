.ourteam-container .swiper-wrapper{
  display: flex;
  padding-left: 10px;
  width: 100%;
}
.ourteam-container .slide-item{
  width: 360px;
  box-sizing: border-box;
  padding: 0 37px;
}
.ourteam-container .swiper-slide-active .is-hidden {
  display: block !important;
  height auto !important
}
.ourteam-container .swiper-slide-active .is-hidden::after{
  display none
}
.ourteam-container .swiper-slide-active .desc{
  //display: -webkit-box;
  //-webkit-box-orient: vertical;
  //-webkit-line-clamp: 7;
  //overflow: hidden;
}
.ourteam-container .swiper-slide.swiper-slide-active .slide-item .look-info{
  display: block !important;
}
.ourteam-container .swiper-slide .slide-item .look-info.isblock {
  display: none !important;
}
.ourteam-container .swiper-slide.swiper-slide-active .slide-item .img-box img{
  //width: 164px;
  //height: 164px;
}
.mobile-wrapper .swiper-wrapper{
  padding-left: 0;
}
.ourteam-container .swiper-button-disabled{
  display: none !important;
}
.mobile-wrapper .swiper-pagination{
  margin-top: 10px;
  position: relative;
  left: 50%;
  transform: translateX(-50%);
  text-align: center;
}
.mobile-wrapper .swiper-pagination-bullet{
  margin: 0 5px;
  cursor: pointer;
  width: 8px;
  height: 8px;
  display: inline-block;
  border-radius: 100%;
  background: #000;
  opacity: .2;
}
.mobile-wrapper .slide-item{
  width: 340px;
}
.mobile-wrapper .swiper-pagination-bullet-active{
  background: #f2bb24;
  opacity: 1;
}
.assetRecite-swiper .swiper-wrapper{
  display: flex;
}
.wiki-swiper .swiper-wrapper{
  display: flex;
}
